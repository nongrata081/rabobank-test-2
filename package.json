{
  "name": "rabobank-assignment",
  "version": "0.2.0",
  "description": "stencil form component",
  "scripts": {
    "commit": "git-cz",
    "enforce:gitflow": "node ./node_modules/.bin/enforce-gitflow-branches",
    "changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "recommended:bump": "conventional-recommended-bump --preset=angular",
    "bdd": "wdio wdio.conf.js",
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest"
  },
  "dependencies": {
    "@stencil/core": "0.16.2",
    "@stencil/router": "0.3.1",
    "@stencil/sass": "^0.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/jest": "24.0.4",
    "@wdio/cli": "5.4.17",
    "chai": "4.2.0",
    "commitizen": "3.0.5",
    "conventional-changelog-cli": "2.0.11",
    "conventional-recommended-bump": "4.0.4",
    "cz-conventional-changelog": "2.1.0",
    "enforce-gitflow-branches": "1.0.8",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "ts-jest": "23.10.4",
    "wdio-cucumber-framework": "2.2.8",
    "wdio-dot-reporter": "0.0.10",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "0.1.5",
    "webdriverio": "4.13.2",
    "workbox-build": "3.4.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run enforce:gitflow"
    }
  },
  "license": "MIT"
}
